<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ZbcGuideApp.SingalStrenght">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Label Text="Hello world x5"></Label>
            <Label x:Name="LoadingLabel" Text="Loading"> </Label>
            <ListView x:Name="ListOfAccessPoints">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding PrintInfo}">
                        </TextCell>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
            <Button Clicked="GetStrenght" Text="Hiiii"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>